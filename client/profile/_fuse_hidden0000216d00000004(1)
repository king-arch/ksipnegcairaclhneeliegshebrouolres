      $(document).ready(function(){
         $("#show_modal_body").fadeOut("slow");
      $("#hide_medical").click(function(){
          var troica2 = $("#fa_eye_medical");
        if(troica2.hasClass('fa-eye-slash')){
      $("#show_modal_body").fadeIn("slow");
        $("#fa_eye_medical").addClass('fa-eye').removeClass('fa-eye-slash');
      }
      else{
       $("#show_modal_body").fadeOut("slow");
         $("#fa_eye_medical").addClass('fa-eye-slash').removeClass('fa-eye'); 
      }     
      }); 
      });
