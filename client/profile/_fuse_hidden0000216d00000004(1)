         $("#show_modal_body_info").fadeOut("slow");
      $("#hide_personal_info").click(function(){
        // alert('inside');
          var troica2 = $("#fa_eye_info");
          // alert(troica2);
      
        if(troica2.hasClass('fa-eye-slash')){
          // alert('without eye');
      $("#show_modal_body_info").fadeIn("slow");
        $("#fa_eye_info").addClass('fa-eye').removeClass('fa-eye-slash');
      }
      else{
        // alert('with eye'); 
       $("#show_modal_body_info").fadeOut("slow");
         $("#fa_eye_info").addClass('fa-eye-slash').removeClass('fa-eye'); 
      }     
      }); 
      });
      
   </script>
